      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.105.2",
    "flow-coverage-report": "^0.6.1",
    "jest-mock-random": "^1.0.2",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.9.0"
